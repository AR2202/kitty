


list1 = list(3,4,2,1,3,3)
list2 = list(4,3,5,3,9,3)
list1sorted = list(1)
pop(list1sorted)
list2sorted = list(1)
pop(list2sorted)
others = list()
emptyNumlist = list(1)
pop(emptyNumlist)

remaining = list1 
while (remaining =/= emptyNumlist) do 
    max = pop(list1)
    pop(list1)
    while (list1 =/= emptyNumlist) do
        i = pop(list1)
        pop(list1)
        if (i > max)
            then others = push(max, others)
            max = i            
        else others = push(i, others)
        endif 
    endwhile
    remaining = others 
    list1 = others 
    others = emptyNumlist
    list1sorted = push(max, list1sorted)
endwhile

remaining = list2
while (remaining =/= emptyNumlist) do 
    max = pop(list2)
    pop(list2)
    while (list2 =/= emptyNumlist) do
        i = pop(list2)
        pop(list2)
        if (i > max)
            then others = push(max, others)
            max = i            
        else others = push(i, others)
        endif 
    endwhile
    remaining = others 
    list2 = others 
    others = emptyNumlist
    list2sorted = push(max, list2sorted)
endwhile

sum = 0

while (list1sorted =/= emptyNumlist) do 
    i = pop(list1sorted)
    pop(list1sorted)
    j = pop(list2sorted)
    pop(list2sorted)
    sumNum = i - j 
    if (sumNum < 0)
        then sumNum =  -1 * sumNum
    endif 
    sum = sum + sumNum
endwhile
print("Day 1 example Part 1:")
print(toText(sum))
